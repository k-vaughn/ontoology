Prefix: : <https://standards.iso.org/iso-iec/5087/-3/ed-1/en/ontology/micromobilitynetwork/>
Prefix: owl: <http://www.w3.org/2002/07/owl#>
Prefix: rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
Prefix: rdfs: <http://www.w3.org/2000/01/rdf-schema#>
Prefix: xml: <http://www.w3.org/XML/1998/namespace>
Prefix: roadnetwork: <https://standards.iso.org/iso-iec/5087/-3/ed-1/en/ontology/roadnetwork/>
Prefix: partwhole: <https://standards.iso.org/iso-iec/5087/-1/ed-1/en/ontology/Mereology/>

Ontology: <https://standards.iso.org/iso-iec/5087/-3/ed-1/en/micromobilitynetwork>

ObjectProperty: partwhole:properPartOf
ObjectProperty: partwhole:hasProperPart

Class: roadnetwork:RoadLane
Class: roadnetwork:RoadLink
Class: roadnetwork:RoadNetwork
Class: roadnetwork:Road
Class: roadnetwork:RoadSection
Class: roadnetwork:RoadSegment

Class: MicromobilityLane
	SubClassOf: 
		roadnetwork:RoadLane
	SubClassOf:
		partwhole:properPartOf only MicromobilityPathSegment
	SubClassOf:
		partwhole:properPartOf min 1 MicromobilityPathSegment
Class: MicromobilityLink
	SubClassOf: 
		roadnetwork:RoadLink
	SubClassOf:
		partwhole:properPartOf only MicromobilityPath or MicromobilityNetwork or MicromobilityPathSection
	SubClassOf:
		partwhole:hasProperPart only MicromobilityPathSegment
	SubClassOf:
		partwhole:hasProperPart min 1 MicromobilityPathSegment
Class: MicromobilityNetwork
	SubClassOf: 
		roadnetwork:RoadNetwork
	SubClassOf:
		partwhole:hasProperPart only MicromobilityLink
	SubClassOf:
		partwhole:hasProperPart min 1 MicromobilityLink
	SubClassOf:
		partwhole:hasProperPart only MicromobilityPath or MicromobilityPathSection
Class: MicromobilityPath
	SubClassOf: 
		roadnetwork:Road
	SubClassOf:
		partwhole:properPartOf only MicromobilityNetwork
	SubClassOf:
		partwhole:hasProperPart only MicromobilityLink
	SubClassOf:
		partwhole:hasProperPart min 1 MicromobilityLink
Class: MicromobilityPathSection
	SubClassOf: 
		roadnetwork:RoadSection
	SubClassOf:
		partwhole:properPartOf only MicromobilityNetwork
	SubClassOf:
		partwhole:hasProperPart only MicromobilityPathSegment or MicromobilityLink
Class: MicromobilityPathSegment
	SubClassOf: 
		roadnetwork:RoadSegment
	SubClassOf:
		partwhole:properPartOf only MicromobilityLink
	SubClassOf:
		partwhole:properPartOf min 1 MicromobilityLink
	SubClassOf:
		partwhole:properPartOf only MicromobilityPathSection
	SubClassOf:
		partwhole:hasProperPart only MicromobilityLane
	SubClassOf:
		partwhole:hasProperPart min 1 MicromobilityLane
